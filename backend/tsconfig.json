{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "outDir": "../bdist",
    "sourceMap": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": false,
    "strictNullChecks": false
  }
}